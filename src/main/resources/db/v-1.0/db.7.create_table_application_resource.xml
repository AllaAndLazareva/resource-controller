<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">


    <changeSet id="create-table-application_resources" author="Lazareva">
        <createTable tableName="application_resources">
            <column name="application_id" type="varchar(64)">
                <constraints foreignKeyName="application_resources_application_id_application_id"
                             references="application(id)"/>
            </column>
            <column name="resources_id" type="varchar(64)">
                <constraints foreignKeyName="application_resources_resource_id_resource_id" references="resource(id)"/>
            </column>

        </createTable>
    </changeSet>
</databaseChangeLog>