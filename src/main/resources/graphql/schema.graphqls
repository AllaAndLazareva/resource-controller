type Query{
    realmById(id:ID):Realm

}
type Realm{
    id:ID!
    description: String
    applications:[Application]

}
type Application{
    id: String!
    name:String
    description: String
    resources:[Resource]
}
type Resource{
    id: String!
    key:String
    applications:[Application]
    value:[VersionData]
    currentValue: VersionData
}

type VersionData{
    id:String!
    version:Version
    value:Value
}
type Value{
    value:String!
    data:String

}

type Version{
    version:String
    description:String
}



